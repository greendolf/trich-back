generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type ThreadsContent {
  /// Could not determine type: the field only had null or empty values in the sample set.
  images String[]
  text   String
}

model sections {
  id   Int    @id @map("_id")
  name String
}

model tags {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model threads {
  id      String         @id @default(auto()) @map("_id") @db.ObjectId
  content ThreadsContent
  id_     Int            @map("id")
  section Int
  tags    String[]
  title   String
}
